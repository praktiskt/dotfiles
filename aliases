# Aliases from praktiskt/dotfiles
alias vim="nvim"
alias open="xdg-open"
alias python="python3"
alias copy="xclip -sel clip"
alias translate="docker run soimort/translate-shell"
alias calc="docker run -it praktiskt/insect"
